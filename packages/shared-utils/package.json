{
  "name": "@viso/shared-base",
  "version": "0.2.0",
  "private": false,
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint --ext ts,tsx,js,jsx src",
    "lint:fix": "eslint --ext ts,tsx src --fix",
    "check": "tsc --noEmit",
    "coverage": "vitest run --coverage",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "build": "viso-build",
    "prepublishOnly": "viso-publish --before",
    "postpublish": "viso-publish --after"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "dependencies": {
    "@viso/types": "^0.1.5",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.4"
  }
}
